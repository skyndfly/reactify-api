        "/api/v1/brands/{id}/models": {
            "get": {
                "tags": [
                    "Brand"
                ],
                "summary": "Get all models by brand id",
                "operationId": "02e37fafa114fc93c956ecca387ae937",
                "parameters": [
                    {
                        "name": "id",
